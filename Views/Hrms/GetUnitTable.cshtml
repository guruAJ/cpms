@using Microsoft.AspNetCore.Http
@model CPMS_AUTO.Models.MSUSNo
@inject IHttpContextAccessor Accessor

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_SupportLayout.cshtml";
}

<script src="~/Auto/js/jquery-1.10.0.min.js" type="text/javascript"></script>
<script src="~/Auto/js/jquery-ui.min.js" type="text/javascript"></script>
<link href="~/Auto/css/jquery-ui.css" rel="Stylesheet" type="text/css" />
<link rel="stylesheet" type="text/css" href="~/css/jquery.dataTables.css">


<header class="page-header page-header-dark bg-gradient-primary-to-secondary pb-10">
    <div class="container">
        <div class="page-header-content pt-4">
            <div class="align-items-center justify-content-between">
                <h1 class="page-header-title">
                    <div>
                        <div class="page-header-icon"><i data-feather="activity"></i></div>
                       Status View
                    </div>
                </h1>
            </div>
        </div>
    </div>
</header>
<style>
    .wrapHeight{
        height:auto;
    }
</style>
<div class="container mt-n10">
    <div class=row>
        <div class="col-12 mb-1">
            <div class="card ">
                <div class="card-body h-100 d-flex flex-column justify-content-center pb-lg-5 pb-xl-4">
                    <div class="row">
                        <div class="col-4 mb-4">
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <div class="form-group1">
                                <label class="control-label" style="margin-left:136px">Project</label><br />
                                <select style="width: 137px; height: 38px;margin-left:136px;" id="proj"><option value="1">HRMS</option><option value="2">IQMP</option></select>
                            </div>
                        </div>

                        <div class="col-4 mb-4">
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <div class="form-group1">
                                <label class="control-label" style="margin-left: 20px;">Status</label><br />
                                <select style="width: 137px; height: 38px; margin-left: 20px;" id="projstatus"><option value="1">Not Installed</option><option value="2">Completely Not Functional</option><option value="3">Partially Functional</option><option value="4">Fully Functional</option></select>
                            </div>
                        </div>
                        <div class="col-4 mt-lg-5">
                            <div class="form-group1">
                                <label class="control-label"></label>
                                <input type="button" value="Search" id="SSearch" class="btn btn-primary" style="width: 100px; margin-top: -29px;margin-left:-29px" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-12 mb-4">
            <div class="card h-100">
                <div class="card-body h-100 d-flex flex-column1 justify-content-center pb-lg-5 pb-xl-4">
                    <div class="table-wrapper-scroll-y my-custom-scrollbar wrapHeight" id="wrapA">
                        <h6 style="text-align: center; margin-left: -242px;" id="hed1"><u>HRMS</u></h6>
                        <table class="table table-bordered table-striped mb-0" id="table_id2" style="width: 709px; margin-left: 217px;height:auto">
                            <thead>
                                <tr>
                                    <th style="width:93px;text-align:center;">
                                        S/No
                                    </th>

                                    <th style="width: 80px; text-align: left;">
                                        Status
                                    </th>

                                    <th style="width: 123px; text-align: center;">
                                        Total Units
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td style="width:93px;text-align:center;">
                                        1
                                    </td>
                                    <td style="width:93px;text-align:left;">
                                        Not Installed
                                    </td>
                                    <td style="width:93px;text-align:center;">
                                        @ViewBag.NotInstalledHrms
                                    </td>
                                </tr>
                                <tr>
                                    <td style="width:93px;text-align:center;">
                                        2
                                    </td>
                                    <td style="width:93px;text-align:left;">
                                        Not Functional
                                    </td>
                                    <td style="width:93px;text-align:center;">
                                        @ViewBag.NotFunctionalHrms
                                    </td>
                                </tr>
                                <tr>
                                    <td style="width:93px;text-align:center;">
                                        3
                                    </td>
                                    <td style="width:93px;text-align:left;">
                                        Partially Functional
                                    </td>
                                    <td style="width:93px;text-align:center;">
                                        @ViewBag.PartiallyFunctionalHrms
                                    </td>
                                </tr>
                                <tr>
                                    <td style="width:93px;text-align:center;">
                                        4
                                    </td>
                                    <td style="width:93px;text-align:left;">
                                        Fully Functional
                                    </td>
                                    <td style="width:93px;text-align:center;">
                                        @ViewBag.FullyFunctionalHrms
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <h6 style="text-align: center; margin-left: -242px;" class="mt-4" id="hed2"><u>IQMP</u></h6>
                        <table class="table table-bordered table-striped mb-0" id="table_id3" style="width: 709px; margin-left: 217px;height:auto">
                            <thead>
                                <tr>
                                    <th style="width:93px;text-align:center;">
                                        S/No
                                    </th>

                                    <th style="width: 80px; text-align: left;">
                                        Status
                                    </th>

                                    <th style="width: 123px; text-align: center;">
                                        Total Units
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td style="width:93px;text-align:center;">
                                        1
                                    </td>
                                    <td style="width: 80px;text-align:left;">
                                        Not Installed
                                    </td>
                                    <td style="width:93px;text-align:center;">
                                        @ViewBag.NotInstalledIqmp
                                    </td>
                                </tr>
                                <tr>
                                    <td style="width:93px;text-align:center;">
                                        2
                                    </td>
                                    <td style="width:93px;text-align:left;">
                                        Not Functional
                                    </td>
                                    <td style="width: 80px;text-align:center;">
                                        @ViewBag.NotFunctionalIqmp
                                    </td>
                                </tr>
                                <tr>
                                    <td style="width:93px;text-align:center;">
                                        3
                                    </td>
                                    <td style="width:93px;text-align:left;">
                                        Partially Functional
                                    </td>
                                    <td style="width: 80px;text-align:center;">
                                        @ViewBag.PartiallyFunctionalIqmp
                                    </td>
                                </tr>
                                <tr>
                                    <td style="width:93px;text-align:center;">
                                        4
                                    </td>
                                    <td style="width:93px;text-align:left;">
                                        Fully Functional
                                    </td>
                                    <td style="width: 80px;text-align:center;">
                                        @ViewBag.FullyFunctionalIqmp
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <table class="table table-bordered table-striped mb-0 hide" id="table_id1" style="width: 709px; margin-left: 217px;">
                            <thead>
                                <tr>
                                    <th style="width:93px;text-align:center;">
                                        S/No
                                    </th>

                                    <th style="width: 180px; text-align: center;">
                                        SUS No
                                    </th>

                                    <th style="width: 340px; text-align: center;">
                                        Unit
                                    </th>

                                    <th id="hhhh" style="text-align: center;" @*style="width: 332px;"*@>
                                        HRMS Status
                                    </th>

                                    <th id="qqqq" style="text-align: center;" @*style="width: 468px;"*@>
                                        IQMP Status
                                    </th>
                                    @*<th>
                                        Action
                                    </th>*@
                                </tr>
                            </thead>
                            <tbody id="tbldata1">
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/js/dataTables.js"></script>
<script src="~/DeveloperJs/Dashboard.js"></script>